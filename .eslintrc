{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "airbnb/hooks",
    // Prettier goes last so it gets the chance to override other configs
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-use-before-define": "off",
    "react/prop-types": "off",
    "react/no-array-index-key": "warn",
    "react/jsx-filename-extension": "off",
    "react/no-danger": "off",
    "import/newline-after-import": "off"
  }
}
